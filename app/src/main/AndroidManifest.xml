<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tutorialsbuzz.tablayoutdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TabFragments.Contratocliente"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TabFragments.ListaContratos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TabFragments.DatosClientes"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TabFragments.Deudas"
            android:screenOrientation="landscape" />
        <activity
            android:name=".TabFragments.Productos"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TabFragments.Componentes"
            android:screenOrientation="landscape" />



        <activity
            android:name=".TabFragments.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>