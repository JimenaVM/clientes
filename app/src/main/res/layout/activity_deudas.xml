<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    android:background="#050505">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:id="@+id/imageView5"
        android:src="@drawable/icio" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="79dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="DATOS DEUDA"
            android:id="@+id/textView20"
            android:textColor="#fcfcfc"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="58dp"
            android:layout_marginStart="58dp"
            android:typeface="sans" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView7"
            android:src="@drawable/deuda"
            android:layout_toStartOf="@+id/editText10"
            android:layout_above="@+id/editText13"
            android:layout_toLeftOf="@+id/editText10" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Nombre:"
            android:id="@+id/textView21"
            android:textColor="#fefdfd"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textStyle="bold"
            android:typeface="sans" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText10"
            android:layout_toRightOf="@+id/textView21"
            android:layout_alignBottom="@+id/textView21"
            android:layout_alignRight="@+id/textView20"
            android:layout_alignEnd="@+id/textView20"
            android:layout_alignTop="@+id/textView21" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Contrato:"
            android:id="@+id/textView22"
            android:textColor="#fdfdfd"
            android:layout_alignBaseline="@+id/textView21"
            android:layout_alignBottom="@+id/textView21"
            android:layout_toRightOf="@+id/editText10"
            android:layout_toEndOf="@+id/editText10"
            android:textStyle="bold"
            android:typeface="sans" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText13"
            android:layout_alignBottom="@+id/textView22"
            android:layout_toRightOf="@+id/textView22" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Total:"
            android:id="@+id/textView23"
            android:textColor="#fefcfc"
            android:layout_alignBottom="@+id/editText13"
            android:layout_toRightOf="@+id/editText13"
            android:layout_toEndOf="@+id/editText13"
            android:layout_marginLeft="93dp"
            android:textStyle="bold"
            android:layout_marginStart="93dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText14"
            android:layout_alignBottom="@+id/textView22"
            android:layout_toRightOf="@+id/textView22"
            android:layout_alignRight="@+id/textView23"
            android:layout_alignEnd="@+id/textView23"
            android:layout_alignTop="@+id/textView22"

            android:typeface="sans" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText25"
            android:layout_alignBottom="@+id/editText10"
            android:layout_toRightOf="@+id/textView23"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/textView23" />

    </RelativeLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="253dp"
        android:background="#ffffff"
        android:layout_weight="0.69">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="272dp">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/editText15"

                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignBottom="@+id/editText15"
                android:background="#f64545"
                android:id="@+id/relativeLayout3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="            PERIODO            "
                    android:id="@+id/textView19"
                    android:textColor="#ffffff"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:textStyle="bold"
                    android:layout_alignParentStart="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="            PRODUCTO           "
                    android:id="@+id/textView24"
                    android:textColor="#ffffff"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView19"
                    android:textStyle="bold"
                    android:layout_toEndOf="@+id/textView19" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="           NÂº SERVICIO           "
                    android:id="@+id/textView27"
                    android:textColor="#ffffff"
                    android:layout_alignTop="@+id/textView24"
                    android:layout_toRightOf="@+id/textView24"
                    android:textStyle="bold"
                    android:layout_toEndOf="@+id/textView24" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="             DEUDA             "
                    android:id="@+id/textView28"
                    android:textColor="#ffffff"
                    android:textStyle="bold"
                    android:layout_alignTop="@+id/textView27"
                    android:layout_toRightOf="@+id/textView27"
                    android:layout_toEndOf="@+id/textView27" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"></RelativeLayout>
            </RelativeLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/relativeLayout3"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <ListView
                    android:layout_width="157dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView3" />

                <ListView
                    android:layout_width="166dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView4" />

                <ListView
                    android:layout_width="142dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView5" />

                <ListView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView6"
                    android:layout_weight="1" />
            </LinearLayout>


        </RelativeLayout>
    </FrameLayout>


</LinearLayout>