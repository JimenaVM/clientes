<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1"
    android:background="#050505">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="78dp"
        android:id="@+id/imageView5"
        android:src="@drawable/icio" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="65dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="COMPONENETES"
            android:id="@+id/textView20"
            android:textColor="#fcfcfc"
            android:textSize="20dp"
            android:layout_gravity="center_horizontal"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="58dp"
            android:layout_marginStart="58dp"
            android:typeface="sans" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView7"
            android:src="@drawable/componentes"
            android:layout_toStartOf="@+id/editText10"
            android:layout_above="@+id/editText13"
            android:layout_toLeftOf="@+id/editText10" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Nombre:"
            android:id="@+id/textView21"
            android:textColor="#fefdfd"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:textStyle="bold"
            android:typeface="sans" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText10"
            android:layout_toRightOf="@+id/textView21"
            android:layout_alignBottom="@+id/textView21"
            android:layout_alignRight="@+id/textView20"
            android:layout_alignEnd="@+id/textView20"
            android:layout_alignTop="@+id/textView21"
            android:typeface="sans" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Contrato:"
            android:id="@+id/textView22"
            android:textColor="#fdfdfd"
            android:layout_alignBaseline="@+id/textView21"
            android:layout_alignBottom="@+id/textView21"
            android:layout_toRightOf="@+id/editText10"
            android:layout_toEndOf="@+id/editText10"
            android:textStyle="bold"
            android:typeface="sans" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText13"
            android:layout_alignBottom="@+id/textView22"
            android:layout_toRightOf="@+id/textView22" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Total:"
            android:id="@+id/textView23"
            android:textColor="#fefcfc"
            android:layout_alignBottom="@+id/editText13"
            android:layout_toRightOf="@+id/editText13"
            android:layout_toEndOf="@+id/editText13"
            android:layout_marginLeft="93dp"
            android:textStyle="bold"
            android:layout_marginStart="93dp" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText14"
            android:layout_alignBottom="@+id/textView22"
            android:layout_toRightOf="@+id/textView22"
            android:layout_alignRight="@+id/textView23"
            android:layout_alignEnd="@+id/textView23"
            android:layout_alignTop="@+id/textView22"

            android:typeface="sans" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editText25"
            android:layout_alignBottom="@+id/editText10"
            android:layout_toRightOf="@+id/textView23"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/textView23" />

    </RelativeLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="253dp"
        android:background="#ffffff"
        android:layout_weight="0.69">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="272dp">


            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/editText15"

                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <GridView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/gridView3"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/editText15" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:background="#f73434"
                android:id="@+id/relativeLayout4"
                android:layout_above="@+id/gridView3">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="        ID COMPONENTE      "
                    android:id="@+id/textView39"
                    android:layout_centerVertical="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:textStyle="bold"
                    android:textColor="#ffffff" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="           TIPO            "
                    android:id="@+id/textView40"
                    android:textColor="#ffffff"
                    android:layout_alignBottom="@+id/textView39"
                    android:layout_toRightOf="@+id/textView39"
                    android:textStyle="bold"
                    android:layout_toEndOf="@+id/textView39" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="              ESTADO               "
                    android:id="@+id/textView41"
                    android:textColor="#ffffff"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView40"
                    android:textStyle="bold"
                    android:layout_toEndOf="@+id/textView40" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="      CLASE SERVICIO    "
                    android:textStyle="bold"
                    android:id="@+id/textView42"
                    android:textColor="#ffffff"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/textView41"
                    android:layout_toEndOf="@+id/textView41" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:id="@+id/textView43"
                    android:layout_alignTop="@+id/textView42"
                    android:layout_toEndOf="@+id/textView42"
                    android:layout_toRightOf="@+id/textView42" />
            </RelativeLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/relativeLayout4"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <ListView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView7" />

                <ListView
                    android:layout_width="124dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView8" />

                <ListView
                    android:layout_width="139dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView9" />

                <ListView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView10"
                    android:layout_weight="1" />
            </LinearLayout>


        </RelativeLayout>
    </FrameLayout>


</LinearLayout>